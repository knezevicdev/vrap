{
  "presets": [
    [
      "next/babel",
      {
        "class-properties": {
          "loose": true
        }
      }
    ],
    "mobx"
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ],
		[
			"@babel/plugin-transform-runtime",
			{
				"regenerator": true,
				"corejs": 3 // or 2; if polyfills needed
			}
		]
  ]
}
