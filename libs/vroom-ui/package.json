{
  "name": "vroom-ui",
  "version": "0.0.0",
  "private": true,
  "files": [
    "dist",
    "public"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.json",
    "storybook": "start-storybook -p 1337 -s public",
    "build:storybook": "build-storybook -s public",
    "build": "yarn clean && yarn build:lib-types && yarn build:lib-source",
    "clean": "rimraf dist/*",
    "build:lib-types": "tsc --project tsconfig.build.json",
    "build:lib-source": "babel src --out-dir dist --extensions '.ts,.tsx'"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.7",
    "@babel/preset-typescript": "^7.12.1",
    "@storybook/addon-essentials": "^6.1.0",
    "@storybook/react": "^6.1.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "babel-loader": "^8.2.1",
    "babel-plugin-inline-react-svg": "^1.1.2",
    "eslint": "^7.13.0",
    "rimraf": "^3.0.2",
    "storybook-addon-designs": "^5.4.2",
    "storybook-addon-performance": "^0.13.0",
    "typescript": "^4.0.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "@researchgate/react-intersection-observer": "^1.3.5"
  }
}
